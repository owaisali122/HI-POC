{
  "name": "poc",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3000",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "seed:forms": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/seed-forms.ts"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.71.1",
    "@payloadcms/next": "3.71.1",
    "@payloadcms/richtext-lexical": "3.71.1",
    "@payloadcms/ui": "3.71.1",
    "bootstrap": "5.3.8",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "formiojs": "^4.21.7",
    "graphql": "^16.8.1",
    "next": "15.4.10",
    "payload": "3.71.1",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "sharp": "0.34.2"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.7",
    "prettier": "^3.4.2",
    "sass": "^1.97.2",
    "tsx": "^4.19.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
